{
  "name": "om-intel-chat",
  "version": "2.2.1",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "_comment": "NODE_OPTIONS='--no-deprecation' suppresses punycode DEP0040 from dependencies tr46/uri-js. Remove when deps updated.",
    "dev:port": "kill -9 $(lsof -ti:3000) 2>/dev/null || true",
    "dev": "npm run dev:port && NODE_OPTIONS='--no-deprecation' next dev --port 3000",
    "dev:clean": "npm run clean && npm run dev",
    "dev:force": "npm run clean:all && npm run dev",
    "dev:alt": "NODE_OPTIONS='--no-deprecation' next dev --port 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --pretty",
    "typecheck:strict": "tsc --noEmit --strict --noImplicitAny --pretty",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check-config": "npx tsx src/scripts/check-config.ts",
    "predev": "npm run check-config",
    "clean": "rm -rf .next && rm -rf node_modules/.cache && rm -rf .turbo",
    "clean:full": "npm run clean && rm -rf out && rm -rf build",
    "clean:all": "rm -rf node_modules .next pnpm-lock.yaml ~/.cache/next-swc || true && pnpm store prune",
    "verify:swc": "node -e \"const v=require('next/package.json').version, p=require.resolve('next/dist/compiled/@next/swc'); console.log('next', v); console.log('swc path', p); console.log('swc pkg', require('next/dist/compiled/@next/swc/package.json').version)\"",
    "clean:lockfiles": "rm -f package-lock.json yarn.lock && pnpm install",
    "clear-cache": "rm -rf .next/cache && rm -rf node_modules/.cache/webpack",
    "prepare": "corepack enable || true"
  },
